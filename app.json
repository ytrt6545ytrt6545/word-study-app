{
  "expo": {
    "name": "Halo Word",
    "slug": "halo-word",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./Vocab.png",
    "scheme": "haloword",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "appName": "Halo Word",
      "versionCode": 2,
      "package": "com.truechitest.haloword",
      "adaptiveIcon": {
        "foregroundImage": "./Vocab.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "react-native-edge-to-edge"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a3289676-1d98-42d6-9300-fd44789d7c8f"
      }
    },
    "owner": "truechitest"
  }
}
